<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.bitmnotify">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CircularActivity"
            android:theme="@style/CircularActivityTheme" />
        <activity
            android:name=".ResourcesActivity"
            android:theme="@style/ResourcesActivityTheme" />
        <activity
            android:name=".ResultActivity"
            android:theme="@style/ResultActivityTheme" />
        <activity
            android:name=".SyllabusActivity"
            android:theme="@style/SyllabusActivityTheme" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>